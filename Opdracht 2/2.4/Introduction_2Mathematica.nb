Cell[TextData[{
 "Here's another example of the use of lists. We will compare the graphs of \
",
 Cell[BoxData[
  FormBox[
   RowBox[{"Sin", "[", "x", "]"}], TraditionalForm]]],
 " with those of its first eight Maclaurin polynomials. Rather than inputting \
the formulas for the polynomials by hand, and rather than use the ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 "  built-in command ",
 StyleBox["Series", "Program",
  FontFamily->"Courier"],
 ", we will build a table of the polynomials all at once. This is done in a \
single line of input to ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 "  (spread out over a number of lines of code below for readability).  Note \
that this line of input contains what would amount to a nested double ",
 StyleBox["Do", "Program"],
 " loop in many other programming languages."
}], "Text",
 CellMargins->{{11, 5}, {Inherited, Inherited}},
 Evaluatable->False,
 AspectRatioFixed->True],

Cell[BoxData[{
 RowBox[{"Clear", "[", "functionlist", "]"}], "\n", 
 RowBox[{
  RowBox[{"functionlist", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"i", "=", "0"}], "n"], 
      FractionBox[
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}], "i"], " ", 
        SuperscriptBox["x", 
         RowBox[{
          RowBox[{"2", " ", "i"}], "+", "1"}]]}], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"2", " ", "i"}], "+", "1"}], ")"}], "!"}]]}], ",", 
     RowBox[{"{", 
      RowBox[{"n", ",", "0", ",", "7"}], "}"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"AppendTo", "[", 
   RowBox[{"functionlist", ",", 
    RowBox[{"Sin", "[", "x", "]"}]}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"colorlist", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"RGBColor", "[", 
      RowBox[{"0", ",", "0", ",", "t"}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", ".1", ",", "1", ",", 
       FractionBox[".9", "7"]}], "}"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"AppendTo", "[", 
   RowBox[{"colorlist", ",", 
    RowBox[{"RGBColor", "[", 
     RowBox[{"1", ",", "0", ",", "0"}], "]"}]}], "]"}], ";"}], "\n", 
 RowBox[{"Plot", "[", 
  RowBox[{
   RowBox[{"Evaluate", "[", "functionlist", "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", "0", ",", "7"}], "}"}], ",", "\n", "\t           ", 
   RowBox[{"PlotStyle", "\[Rule]", "colorlist"}]}], "]"}]}], "Input",
 CellMargins->{{11, 5}, {Inherited, Inherited}},
 AspectRatioFixed->True,
