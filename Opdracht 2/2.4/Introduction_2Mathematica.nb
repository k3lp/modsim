Cell["\<\
A Lissajous figure is a path traced out in the plane by a particle each of \
whose coordinates are under simple harmonic motion. Such trajectories are \
often encountered in physics. The first set of parametric equations of the \
circle in the previous subsection is an example of such motion. Let's try \
some other examples which generate Lissajous figures.\
\>", "Text",
 CellMargins->{{11, 5}, {Inherited, Inherited}},
 Evaluatable->False,
 AspectRatioFixed->True],

Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{"x", ",", "y"}], "]"}], "\n", 
 RowBox[{
  RowBox[{"x", "[", "t_", "]"}], ":=", 
  RowBox[{"Cos", "[", 
   RowBox[{"3", " ", "t"}], "]"}]}], "\n", 
 RowBox[{
  RowBox[{"y", "[", "t_", "]"}], ":=", 
  RowBox[{"Sin", "[", 
   RowBox[{"2", " ", "t"}], "]"}]}], "\n", 
 RowBox[{"ParametricPlot", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"x", "[", "t", "]"}], ",", 
     RowBox[{"y", "[", "t", "]"}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"t", ",", "0", ",", 
     RowBox[{"2", " ", "\[Pi]"}]}], "}"}]}], "]"}]}], "Input",
 CellMargins->{{11, 5}, {Inherited, Inherited}},
 AspectRatioFixed->True,
 CellLabel->"In[251]:="],

Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{"x", ",", "y"}], "]"}], "\n", 
 RowBox[{
  RowBox[{"x", "[", "t_", "]"}], ":=", 
  RowBox[{"Cos", "[", 
   RowBox[{"3", " ", "t"}], "]"}]}], "\n", 
 RowBox[{
  RowBox[{"y", "[", "t_", "]"}], ":=", 
  RowBox[{"Cos", "[", 
   RowBox[{
    RowBox[{"4", " ", "t"}], "+", 
    FractionBox["\[Pi]", "4"]}], "]"}]}], "\n", 
 RowBox[{"ParametricPlot", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"x", "[", "t", "]"}], ",", 
     RowBox[{"y", "[", "t", "]"}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"t", ",", "0", ",", 
     RowBox[{"2", " ", "\[Pi]"}]}], "}"}]}], "]"}]}], "Input",
 CellMargins->{{11, 5}, {Inherited, Inherited}},
